      <!-- Vastuse manuse struktuur -->
      <!-- Vastuses on toodud kõik valdkonnad, mis rahuldavad vähemalt ühte sisendi kriteeriumitest, ning nende alamvaldkonnad.
           Sisendi kriteeriumiks võivad olla valdkonna id väärtused, valdkondade täpsed nimetused (case-insensitive),
           ning valdkondade nimetuste SQL patternid. Juhul kui on vajadus saada täis nimekiri, siis tuleb määrata valdkonnad->id='ALL'. -->
      <xs:complexType name="valdkonnadVastusManus">
        <xs:all>
          <xs:element name="valdkonnad" minOccurs="0" maxOccurs="1">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="valdkond" minOccurs="0" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="id" type="xs:string" minOccurs="1" maxOccurs="1"/>
                      <xs:element name="nimi" type="xs:string" minOccurs="1" maxOccurs="1"/>
                      <xs:element name="nimi_en" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="nimi_ru" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="vanem_id" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="vanem_nimi" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="vanem_nimi_en" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="vanem_nimi_ru" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="kirjeldus" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="kirjeldus_en" type="xs:string" minOccurs="0" maxOccurs="1"/>
                      <xs:element name="kirjeldus_ru" type="xs:string" minOccurs="0" maxOccurs="1"/>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>

      <xs:complexType name="grupidParing">
        <xs:all>
          <xs:element name="kriteeriumid" minOccurs="0">
