                          <xs:appinfo>
                            <xrd:title>Grupi moodustanud asutuse nimetus</xrd:title>
                          </xs:appinfo>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="nimetus" type="xs:string" minOccurs="1">
                        <xs:annotation>
                          <xs:appinfo>
                            <xrd:title>Grupi nimetus</xrd:title>
                          </xs:appinfo>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="kirjeldus" type="xs:string" minOccurs="0" nillable="true">
